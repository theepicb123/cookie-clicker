    scoreshown.content = score;
};

clickerBackup.onMouseDown = function () {
    'use strict';
    score = score + (Math.pow(2, clickerUpgradeRank1) * power);
    scoreshown.content = score;
};

      
function onFrame(event) {
    'use strict';
    timer = timer + 1;
    if (timer === 60) {
        timer = 0;
        score = (score + autoPower);
    }
    scoreshown.content = (score);
    autoPower = (autoUpgrade1 * (Math.pow(2, autoUpgradeRank - 1))) * (Math.pow(2, autoUpgradeOtherRank - 1)) + ((2 * autoUpgrade2) * Math.pow(2, autoUpgradeRank2 - 1));
