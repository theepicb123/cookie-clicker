//upgrade variables


//first rank upgrade {

var rankUpgradeSquareAuto1 = new Shape.Rectangle(90, 480, 30, 30);
rankUpgradeSquareAuto1.fillColor = "orange";


rankUpgradeSquareAuto1.onMouseEnter = function () {
    'use strict';
    infoSquare.bringToFront();
